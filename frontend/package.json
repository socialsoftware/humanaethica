{
  "name": "humanaethica-frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "vue-cli-service serve --fix",
    "build": "NODE_ENV=production vue-cli-service build --mode production",
    "build:dev": "NODE_ENV=development vue-cli-service build --mode development",
    "lint": "vue-cli-service lint",
    "prettier": "prettier --single-quote --write \"**/**/*.{ts,js,css,vue}\"",
    "codecov": "codecov",
    "cypress": "npx cypress open",
    "test:e2e": "npx cypress run"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "~6.5.1",
    "axios": "~1.6.4",
    "glob-all": "^3.3.0",
    "material-design-icons-iconfont": "~6.7.0",
    "node-polyfill-webpack-plugin": "^2.0.1",
    "pg": "~8.11.3",
    "sanitize-html": "~2.12.1",
    "showdown": "~2.1.0",
    "typeface-roboto": "~1.1.13",
    "vue": "~2.7.16",
    "vue-class-component": "~7.2.6",
    "vue-ctk-date-time-picker": "~2.5.0",
    "vue-property-decorator": "~9.1.2",
    "vue-router": "~3.6.5",
    "vue-underscore": "~0.1.4",
    "vuedraggable": "~2.24.3",
    "vuetify": "~2.7.1",
    "vuex": "~3.6.2"
  },
  "devDependencies": {
    "@cypress/vue": "^2.2.3",
    "@mdi/font": "~5.9.55",
    "@types/node": "^22.13.10",
    "@types/plotly.js-dist-min": "~2.3.4",
    "@typescript-eslint/eslint-plugin": "~5.62.0",
    "@typescript-eslint/parser": "~5.62.0",
    "@vue/cli-plugin-babel": "~5.0.8",
    "@vue/cli-plugin-eslint": "~5.0.8",
    "@vue/cli-plugin-pwa": "~5.0.8",
    "@vue/cli-plugin-router": "~5.0.8",
    "@vue/cli-plugin-typescript": "~5.0.8",
    "@vue/cli-plugin-vuex": "~5.0.8",
    "@vue/cli-service": "~5.0.8",
    "@vue/eslint-config-prettier": "~8.0.0",
    "@vue/eslint-config-typescript": "~12.0.0",
    "codecov": "~4.0.0-0",
    "compression-webpack-plugin": "~10.0.0",
    "cypress": "^13.17.0",
    "eslint": "~8.56.0",
    "eslint-plugin-prettier": "~5.1.2",
    "eslint-plugin-vue": "~9.19.2",
    "prettier": "~3.1.1",
    "purgecss-webpack-plugin": "~5.0.0",
    "sass": "~1.32.13",
    "sass-loader": "~13.3.3",
    "terser-webpack-plugin": "~5.3.10",
    "typescript": "~5.1.6",
    "vue-cli-plugin-vuetify": "~2.5.8",
    "vue-template-compiler": "~2.6.14",
    "vuetify-loader": "~1.9.2",
    "webpack": "~5.88.1",
    "webpack-bundle-analyzer": "~4.10.1s"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "@vue/prettier",
      "@vue/typescript"
    ],
    "rules": {
      "no-console": 0,
      "error": "off",
      "no-debugger": 0,
      "quotes": [
        "error",
        "single"
      ]
    },
    "parserOptions": {
      "parser": "@typescript-eslint/parser"
    },
    "overrides": []
  },
  "prettier": {
    "singleQuote": true
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "engines": {
    "node": ">=20.14.0",
    "npm": ">=10.2.4"
  },
  "license": "MIT"
}
